{
  "layout": "minimal",
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "variant_picker": {
          "type": "variant_picker",
          "settings": {
            "hide_sold_out_variants": false,
            "selector_mode": "block",
            "color_mode": "color",
            "size_chart_page": ""
          }
        },
        "quantity_selector": {
          "type": "quantity_selector",
          "settings": {
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_payment_button": true,
            "atc_button_background": "rgba(0,0,0,0)",
            "atc_button_text_color": "rgba(0,0,0,0)",
            "buy_now_button_background": "rgba(0,0,0,0)",
            "buy_now_button_text_color": "rgba(0,0,0,0)"
          }
        }
      },
      "block_order": [
        "variant_picker",
        "quantity_selector",
        "buy_buttons"
      ],
      "settings": {
        "show_vendor": true,
        "show_sku": false,
        "show_taxes_included": false,
        "show_product_rating": true,
        "show_sticky_add_to_cart": true,
        "show_share_buttons": true,
        "help_page": "",
        "enable_video_autoplay": true,
        "enable_video_looping": false,
        "enable_image_zoom": true,
        "show_thumbnails_on_mobile": false,
        "desktop_thumbnails_position": "bottom",
        "transition_effect": "slide",
        "container_size": "medium",
        "desktop_media_layout": "gallery",
        "gallery_first_line_items_size": "3",
        "gallery_default_line_items_size": "2",
        "gallery_grid_gap": "medium",
        "gallery_size": "2",
        "gallery_no_margin_left": true,
        "gallery_margin_right": "large",
        "tabs_type": "tabs",
        "tabs_position": "below",
        "tabs_behaviour": "first_open",
        "tabs_container_size": "small",
        "tabs_first_grid_item_size": "2"
      }
    }
  },
  "order": [
    "main"
  ]
}