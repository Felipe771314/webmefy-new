    "0a2fdb05-a93c-4937-8e7a-bad588778bcb": {
      "type": "image-with-text",
      "blocks": {
        "template--18018392277278__0a2fdb05-a93c-4937-8e7a-bad588778bcb-1677226317112ed6ec-0": {
