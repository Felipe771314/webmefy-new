{{ 'navbar.scss' | asset_url | stylesheet_tag }}

{%- style -%}
.navbar {
  padding: 20px;
}
{%- endstyle -%}

{%- liquid
  assign title = section.settings.title
-%}

<!-- Navbar -->
<div class="navbar section-{{ section.id }}">
  {{ title }}
</div>

{% schema %}
{
  "name": " Navbar",
  "tag": "section",
  "class": "section-navbar",
  "settings": [],
  "blocks": [],
  "presets": [
    {
      "name": " Navbar",
      "category": "Custom",
      "blocks": []
    }
  ]
}
{% endschema %}